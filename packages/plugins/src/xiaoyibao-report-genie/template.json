{
  "author": "吕老师/Sam",
  "version": "v1",
  "name": "小胰宝报告精灵",
  "avatar": "plugins/xiaoyibao-report-genie/xiaoyibao.png",
  "intro": "对于上传的病情报告进行识别，解析和提取关键指标如CA199，持久化存储并结合智能分析，生成报告，并返回给用户",
  "inputExplanationUrl": "https://uei55ql5ok.feishu.cn/wiki/Yb6LwrowpioqfTkktPVcADnanEf", //飞书设计文档
  "showStatus": true,
  "weight": 10,

  "isTool": true,
  "templateType": "tools",

  "workflow": {
    "nodes": [
      {
        "nodes": [
          {
            "nodeId": "randomNodeId1",
            "name": "图片上传并调用大模型 API",
            "intro": "将图片的 Base64 URL 发送给指定的大模型 API 地址 (https://api.stepfun.com/v1)，并获取解析结果。",
            "avatar": "/imgs/workflow/input.png",
            "flowNodeType": "pluginInput",
            "showStatus": false,
            "version": "481",
            "inputs": [],
            "outputs": ["result"],
            "apiUrl": "https://api.stepfun.com/v1",
            "method": "POST",
            "headers": {},
            "body": {
              "model": "step-1v-8k",
              "messages": [
                {
                  "role": "system",
                  "content": "你是由阶跃星辰提供的AI聊天助手，你除了擅长中文，英文，以及多种其他语言的对话以外，还能够根据用户提供的图片，对内容进行精准的内容文本描述。在保证用户数据安全的前提下，你能对用户的问题和请求，作出快速和精准的回答。同时，你的回答和建议应该拒绝黄赌毒，暴力恐怖主义的内容"
                },
                {
                  "role": "user",
                  "content": [
                    {
                      "type": "text",
                      "text": "用优雅的语言描述这张图片"
                    },
                    {
                      "type": "image_url",
                      "image_url": {
                        "url": "data:image/webp;base64,\${input.base64Url}"
                      }
                    }
                  ]
                }
              ]
            }
          },
          {
            "nodeId": "randomNodeId2",
            "name": "处理解析结果",
            "intro": "处理从大模型 API 返回的解析结果，并提取日期和 ca199 变量值。",
            "avatar": "/imgs/workflow/output.png",
            "flowNodeType": "pluginOutput",
            "showStatus": false,
            "version": "481",
            "inputs": ["result"],
            "outputs": ["extractedData"],
            "process": {
              "extractDateAndCa199": true
            }
          },
          {
            "nodeId": "randomNodeId3",
            "name": "存储数据到 MongoDB",
            "intro": "将提取的数据存储到 MongoDB 中。",
            "avatar": "/imgs/workflow/output.png",
            "flowNodeType": "pluginOutput",
            "showStatus": false,
            "version": "481",
            "inputs": ["extractedData"],
            "outputs": ["storedData"]
          },
          {
            "nodeId": "randomNodeId4",
            "name": "计算变动值和绘制折线图",
            "intro": "计算 ca199 在两个不同日期的变动值和变动比例，并绘制折线图。",
            "avatar": "/imgs/workflow/output.png",
            "flowNodeType": "pluginOutput",
            "showStatus": false,
            "version": "481",
            "inputs": ["storedData"],
            "outputs": ["chartData"]
          }
        ],
        "edges": [
          {
            "source": "randomNodeId1",
            "target": "randomNodeId2",
            "sourceHandle": "result",
            "targetHandle": "result"
          },
          {
            "source": "randomNodeId2",
            "target": "randomNodeId3",
            "sourceHandle": "extractedData",
            "targetHandle": "extractedData"
          },
          {
            "source": "randomNodeId3",
            "target": "randomNodeId4",
            "sourceHandle": "storedData",
            "targetHandle": "storedData"
          }
        ]
      }  
    ]  
  }
}